@model IEnumerable<NTTool.Models.MachineEntity>

@{
    ViewBag.Title = "Scan";
}

<h2>Scan</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MachineName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MachineName)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.MachineName}) |
        </td>
    </tr>
}

</table>
